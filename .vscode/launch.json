{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug config tools webview",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/ide"],
			"outFiles": ["${workspaceFolder}/packages/ide/out/**/*.js"],
			"preLaunchTask": "cfgtools: build"
		},
		{
			"name": "Run IDE Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/ide"],
			"outFiles": ["${workspaceFolder}/packages/ide/out/**/*.js"],
			"preLaunchTask": "ide: build"
		},
		{
			"name": "Debug ELF file explorer webview",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/ide"],
			"outFiles": ["${workspaceFolder}/packages/ide/out/**/*.js"],
			"preLaunchTask": "elf: build"
		},
		{
			"name": "Debug workspace creation webview",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/ide"],
			"outFiles": ["${workspaceFolder}/packages/ide/out/**/*.js"],
			"preLaunchTask": "wrksp: build"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Execute CLI Dev Command",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/packages/pinconfig-cli/bin/dev.js",
			"args": ["hello", "world"],
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsx",
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}
