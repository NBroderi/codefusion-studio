{
  "author": "Analog Devices, Inc.",
  "bin": "./bin/run.js",
  "description": "ADI CodeFusion Studio CLI tool",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4",
    "cfs-lib": "workspace:*",
    "cfs-plugins-api": "workspace:*",
    "cli-table3": "^0.6.5",
    "elf-parser": "workspace:*",
    "eslint-plugin-import": "^2.29.1",
    "eta": "^3.5.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/copyfiles": "^2",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "copyfiles": "^2.4.1",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "3.0.38",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": "18.15.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/analogdevicesinc/codefusion-studio/tree/V1.1.0/packages/cli",
  "license": "MIT",
  "main": "",
  "name": "cfsutil",
  "oclif": {
    "bin": "cfsutil",
    "dirname": "cfsutil",
    "commands": "./dist/commands",
    "hooks": {
      "get-data-models": "./dist/hooks/get-data-models/provide-data-models"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "elf": {
        "description": "ELF parsing library."
      },
      "dt": {
        "description": "Devicetree parser."
      },
      "cfsplugins": {
        "description": "CFS plugins."
      },
      "workspace": {
        "description": "Workspace."
      },
      "project": {
        "description": "Project."
      }
    }
  },
  "repository": "https://github.com/analogdevicesinc/codefusion-studio/tree/V1.1.0/packages/cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && copyfiles -u 1 'src/**/*.json' dist/",
    "build-local-deps": "yarn ws:lib build && yarn ws:elf-parser build",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "tarballs": "yarn build-local-deps && node build-scripts/tarballs.js && oclif pack tarballs",
    "test": "mocha --forbid-only \"test/commands/**/*.test.ts\"",
    "test-plugins": "mocha --forbid-only \"test/plugins/**/*.test.ts\"",
    "test-gen": "mocha --forbid-only \"test/commands/generate.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "1.1.0-25",
  "bugs": "https://github.com/analogdevicesinc/codefusion-studio/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
