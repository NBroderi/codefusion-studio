{
  "Copyright": "Copyright (c) <%= it.copyrightDate %> Analog Devices, Inc.  All rights reserved. This software is proprietary to Analog Devices, Inc. and its licensors.",
  "DataModelVersion": "<%= it.dataModelVersion %>",
  "DataModelSchemaVersion": "<%= it.dataModelSchemaVersion %>",
  "Soc": "<%= it.soc %>",
  "Package": "<%= it.package %>",
  "BoardName": "<%= it.board %>",
  "Timestamp": "<%= new Date().toISOString() %>",
  "Projects": [
<% if (it.projects && it.projects.length) { %>
  <% it.projects.forEach((project, index) => { %>
  {
      "CoreId": "<%= project.id %>",
      "ProjectId": "<%= project.id %>",
      "PluginId": "<%= project.pluginId %>",
      "PluginVersion": "<%= project.pluginVersion %>",
      "FirmwarePlatform": "<%= project.firmwarePlatform %>",
      "ExternallyManaged": <%= project.isEnabled %>,
      "PlatformConfig": {

<%_ if (project.platformConfig && Object.keys(project.platformConfig).length > 0) { %>
        <%= Object.keys(project.platformConfig)
            .map(key => `        "${key}": ${JSON.stringify(project.platformConfig[key])}`)
            .join(',\n') %>
<% } %>

      },
      "Partitions": [],
      "Peripherals": []
    }<% if (index < it.projects.length - 1) { %>,
<% } %>
  <% }) %>
<% } _%> <%-%>

  ],
  "Pins": [],
  "ClockNodes": []
}
